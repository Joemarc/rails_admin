%fieldset.radio_buttons.medium{data: {bind: 'foreach: times'}}
  = label_tag '', '', data: {bind: 'attr: {for: "service_time_"+time}'} do
    = form.radio_button :time, {}, data: {bind: 'value: time, attr: {id: "service_time_"+time}'}
    %span{data: {bind: 'text: friendly_time'}, class: "action-button-transparent size-medium"}
